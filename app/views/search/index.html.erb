<div class = "container">
  <h1>List of most popular events for March 2017</h1>
  <h2>Top 16</h2>
  <table>
    <tr>
      <th>Event name</th>
      <th>Date & Time</th>
      <th>Description</th>
      <th>Venue name</th>
      <th>Venue address</th>
    </tr>
    <% @events.each do |event|%>
      <tr>
        <td><%= event. %></td>
      </tr>
    <% end %>
  </table>
</div>